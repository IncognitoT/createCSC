 import numpy as np

arr1 = np.arange(1,37).reshape(6,6)

-----------------------------------------------------------
subarr1  = arr1[[1, 2, 3,4], :][:, [2, 1, 3,4]]

----------------------------------------------------------
def vandermonde(n):
    
    
    table = [[0 for c in range(n)] for r in range(n)]
    
    for i in range(n):
        
        for k in range(n):
            
            table[i][k] = (i+1)**k
            
    return table
 --------------------------------------------------------------
 arr3 = vandermonde(6)
 for i in range(len(arr3)):
        
    for k in range(len(arr3[0])):
            
        if arr3[i][k] % 2 !=0 :
            
            arr3[i][k] = -1
