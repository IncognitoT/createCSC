def signature(text):
    
    strToConcat = ' EPITA.'
    
    opText = text + strToConcat
    
    return opText
-------------------------------------------------------
def delete_signature(text):
    
    strToDel = ' EPITA.'
    
    OpStr = ''
    
    if strToDel in text:
        
        OpStr = text.replace(strToDel,'')
        
        return OpStr
        
    else:
        
        return text
