list_ = [i**2 for i in range(1,21)]
----------------------------------------
def inter_in(l, i, j):
    
    temp = l[i]
    l[i] = l[j]
    l[j] = temp
    
    return l
    
----------------------------------------------
def inter_out(l, i, j):
    
    copyVar = l.copy()
    
    temp = copyVar[i]
    copyVar[i] = copyVar[j]
    copyVar[j] = temp
    
    return copyVar
    
